<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval" >
            <solid android:color="@color/beed_border" />
        </shape>
    </item>
    <item
        android:bottom="@dimen/line_padding"
        android:right="@dimen/line_padding"
        android:top="@dimen/line_padding">
        <shape
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle" >
            <solid android:color="@color/beed_circle_line" />
        </shape>
    </item>
    <item
        android:bottom="@dimen/beeds_border"
        android:left="@dimen/beeds_border"
        android:right="@dimen/beeds_border"
        android:top="@dimen/beeds_border">
        <shape
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval" >
            <solid android:color="@color/beed_not_selected_circle" />
        </shape>
    </item>

</layer-list>